    "def solution(n, k):\r\n",
    "    pk = []\r\n",
    "    re = ''\r\n",
    "    while n > 0:\r\n",
    "        n, m = divmod(n, k)\r\n",
    "        re += str(m)\r\n",
    "    re = re[::-1] # k진수 변환 값\r\n",
    "    \r\n",
    "    a = re.index(\"0\") # 첫 0\r\n",
    "    pk.append(re[:a])\r\n",
    "    t = re[a+3]\r\n",
    "\r\n",
    "    return t\r\n",
    "solution(437674,3)"
